fn super_max(cmp: Int -> Int -> Bool, x: Int, y: Int) do
    if cmp(x, y) do
        x
    else
        y
    end
end

fn greater(x: Int, y: Int) do
    !(x <= y)
end

fn rec meaning_of_universe(x: Int): Int do
    if x >= 42 do
        0
    else
       1 + meaning_of_universe(x + 1)
    end
end

max = super_max(greater)
max(meaning_of_universe(0),- 10)
